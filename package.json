{
	"name": "deepfunding-public-juror",
	"version": "0.1.0",
	"description": "Deep Funding Public Juror Data Collection App",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"prebuild": "node scripts/set-version.js && node scripts/generate-datasets.js",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"generate-datasets": "node scripts/generate-datasets.js",
		"generate-elo": "node scripts/generate-datasets.js",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"deploy:preview": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env preview",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
		"kv": "node scripts/kv-manager.js",
		"setup-ai-db": "node scripts/setup-ai-comparisons-db.js",
		"setup-ai-db:preview": "node scripts/setup-ai-comparisons-db.js --env=preview",
		"populate-ai-comparisons": "node scripts/populate-ai-comparisons.js"
	},
	"dependencies": {
		"@rainbow-me/rainbowkit": "^2.2.9",
		"@tanstack/react-query": "^5.90.5",
		"iron-session": "^8.0.4",
		"next": "15.4.6",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"siwe": "^3.0.0",
		"viem": "^2.38.5",
		"wagmi": "^2.19.1"
	},
	"devDependencies": {
		"@cloudflare/next-on-pages": "^1.13.16",
		"@eslint/eslintrc": "^3",
		"@opennextjs/cloudflare": "^1.11.0",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"wrangler": "^4.45.2"
	}
}
